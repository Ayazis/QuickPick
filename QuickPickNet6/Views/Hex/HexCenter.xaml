<UserControl x:Class="QuickPick.UI.Views.Hex.HexCenter"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:hex="clr-namespace:QuickPick.UI.Views.Hex"
             xmlns:local="clr-namespace:QuickPick.UI.Views.Hex"
             mc:Ignorable="d" 
             Width="auto"
             Height="auto"
             d:DesignHeight="125" d:DesignWidth="125">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="..\Resources.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid Width="125" Height="125">
        <Canvas Width="125" Height="125" Name="HexCanvas" HorizontalAlignment="Center" VerticalAlignment="Center"/>
    </Grid>
</UserControl>
